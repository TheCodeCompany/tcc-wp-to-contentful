# WordPress to Contentful Migration Configuration
# Copy this file to .env and fill in your actual values
# .env is excluded from version control for security

# WordPress Configuration
WP_ENDPOINT=https://your-site.com/wp-json/wp/v2/
WP_IMPORT_POST_COUNT=2

# Contentful Configuration
CONTENTFUL_ACCESS_TOKEN=CFPAT-your-contentful-access-token-here
CONTENTFUL_SPACE_ID=your-space-id-here
CONTENTFUL_ENVIRONMENT=master
CONTENTFUL_CONTENT_TYPE=blogPost
